<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="toplevel1">
    <property name="height">200</property>
    <property name="minsize">500|100</property>
    <property name="resizable">horizontally</property>
    <property name="width">200</property>
    <containerlayout manager="grid">
      <property type="col" id="0" name="weight">1</property>
    </containerlayout>
    <child>
      <object class="customtkinter.CTkFrame" id="ctkframe1" named="True">
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">0</property>
          <property name="sticky">nsew</property>
        </layout>
        <containerlayout manager="grid">
          <property name="anchor">nw</property>
          <property type="col" id="0" name="weight">1</property>
          <property type="row" id="0" name="weight">1</property>
        </containerlayout>
        <child>
          <object class="customtkinter.CTkFrame" id="ctkframe2">
            <layout manager="grid">
              <property name="column">0</property>
              <property name="columnspan">3</property>
              <property name="row">0</property>
              <property name="sticky">nsew</property>
            </layout>
            <containerlayout manager="grid">
              <property type="col" id="0" name="weight">1</property>
            </containerlayout>
            <child>
              <object class="customtkinter.CTkLabel" id="ctklabel1">
                <property name="text" translatable="yes">Data:</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="padx">10</property>
                  <property name="pady">0</property>
                  <property name="row">0</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="customtkinter.CTkLabel" id="ctklabel2">
                <property name="text" translatable="yes">Groups:</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="ipady">0</property>
                  <property name="padx">10</property>
                  <property name="pady">0</property>
                  <property name="row">1</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="customtkinter.CTkTextbox" id="group_field" named="True">
                <property name="state">active</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="padx">10</property>
                  <property name="pady">0</property>
                  <property name="row">2</property>
                  <property name="sticky">nsew</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="customtkinter.CTkLabel" id="ctklabel3">
                <property name="text" translatable="yes">Add another group</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="padx">10</property>
                  <property name="pady">0</property>
                  <property name="row">3</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="customtkinter.CTkComboBox" id="sel_group" named="True">
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="padx">10</property>
                  <property name="pady">5</property>
                  <property name="row">4</property>
                  <property name="sticky">ew</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="customtkinter.CTkButton" id="btn_to_existing" named="True">
            <property name="command" type="command" cbtype="simple">assign_to_existing</property>
            <property name="text" translatable="yes">Assign to group</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">10</property>
              <property name="pady">10</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="customtkinter.CTkButton" id="btn_new" named="True">
            <property name="command" type="command" cbtype="simple">assign_to_new</property>
            <property name="text" translatable="yes">Create new group</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">10</property>
              <property name="pady">10</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="customtkinter.CTkButton" id="btn_cancel" named="True">
            <property name="command" type="command" cbtype="simple">cancel</property>
            <property name="text" translatable="yes">Cancel</property>
            <layout manager="grid">
              <property name="column">2</property>
              <property name="padx">10</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
